    {
      "parameters": {
        "jsCode": "const input = $input.all();\n\n// Check if this is a single file or multiple files\nif (input.length === 1) {\n  // Single file summary\n  const item = input[0].json;\n  const summary = item.output || item.text || 'Could not generate summary';\n  const fileName = item.fileName;\n  const folderPath = item.folderPath;\n  \n  const responseMessage = `ðŸ“„ *SUMMARY*\\n\\n` +\n    `ðŸ“ Folder: /${folderPath || 'root'}\\n` +\n    `ðŸ“„ File: ${fileName}\\n\\n` +\n    `ðŸ“ *Summary:*\\n${summary}`;\n  \n  return [{\n    json: {\n      message: item.originalMessage,\n      fileList: responseMessage\n    }\n  }];\n  \n} else {\n  // Multiple files - create bullet digest\n  const folderPath = input[0].json.folderPath;\n  const summaries = input.map(item => {\n    const summary = item.json.output || item.json.text || 'Could not summarize';\n    return `â€¢ **${item.json.fileName}**: ${summary}`;\n  }).join('\\n\\n');\n  \n  const responseMessage = `ðŸ“„ *SUMMARY of /${folderPath}:*\\n\\n${summaries}`;\n  \n  return [{\n    json: {\n      message: input[0].json.originalMessage,\n      fileList: responseMessage\n    }\n  }];\n}"
      },
